{
    "name": "nshp/laravel-session",
    "description": "A custom Laravel session handler",
    "type": "composer-plugin",
    "version": "1.0.0", 
    "authors": [
        {
            "name": "Hồ Tiến Kỷ",
            "email": "hothanh1072@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nshp\\LaravelSession\\": "src/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/collections": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "symfony/finder": "^5.4",
        "symfony/http-foundation": "^5.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "illuminate/console": "Required to use the session:table command (^7.4)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/nshp/"},
        {"packagist.org": false}
    ]
}
